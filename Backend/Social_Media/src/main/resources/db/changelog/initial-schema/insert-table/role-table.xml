<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

        <changeSet id="51" author="Jemis">
            <insert tableName="role">
                <column name="role_id" value="1"/>
                <column name="role_name" value="SIMPLEUSER"/>
                <column name="default_role" value="true"/>
                <column name="created_date" valueComputed="CURRENT_TIMESTAMP"/>
                <column name="modified_date" valueComputed="CURRENT_TIMESTAMP"/>
            </insert>
        </changeSet>

</databaseChangeLog>