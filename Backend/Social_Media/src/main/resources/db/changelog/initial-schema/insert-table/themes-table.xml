<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

        <changeSet id="52" author="Jemis">
            <insert tableName="themes">
                <column name="theme_id" value="1"/>
                <column name="theme_name" value="dark"/>
                <column name="primary_color" value="#24759b"/>
                <column name="secondary_color" value="#f48fb1"/>
                <column name="background_color" value="#121212"/>
                <column name="text_color" value="#E0E0E0"/>
                <column name="sub_background_color" value="#2b2b2b"/>
                <column name="sub_text_color" value="#24759b"/>
                <column name="button_background_color" value="#24759b"/>
                <column name="button_text_color" value="#E0E0E0"/>
                <column name="anchor_text_color" value="#E0E0E0"/>
                <column name="text_field_color" value="#E0E0E0"/>
                <column name="created_user" value="0"/>
                <column name="modification" value="false"/>
                <column name="granted_users" value="NULL"/>
                <column name="created_date" valueComputed="CURRENT_TIMESTAMP"/>
                <column name="modified_date" valueComputed="CURRENT_TIMESTAMP"/>
            </insert>
        </changeSet>

</databaseChangeLog>